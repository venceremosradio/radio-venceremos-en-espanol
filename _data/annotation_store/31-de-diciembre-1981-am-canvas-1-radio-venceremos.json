        }
      }
    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "31-de-diciembre-1981-am-canvas-1-radio-venceremos-annotation-10.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Hermanos unidos para combatir. Avancemos a la revolución. Construyamos un futuro de progreso y dignidad. Revolución o muerte, ¡venceremos!",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-espanol/31-de-diciembre-1981-am/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
          "t": "432.0,454.0"
        }
      }
    },
