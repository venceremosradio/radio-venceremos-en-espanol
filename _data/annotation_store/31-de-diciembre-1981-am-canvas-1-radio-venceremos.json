        }
      }
    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "31-de-diciembre-1981-am-canvas-1-radio-venceremos-annotation-9.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Farabundo Martí, vienes rompiendo caminos libertarios por donde transitarán la paz y la justicia de nuestro pueblo.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-espanol/31-de-diciembre-1981-am/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
