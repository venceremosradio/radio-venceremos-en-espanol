      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Nuestro apartado postal, 7-907, México, Distrito Federal.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-espanol/31-de-diciembre-1981-am/canvas-1/canvas",
