    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "31-de-diciembre-1981-pm-canvas-1-radio-venceremos-annotation-4.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Hemos escuchado el mensaje de fin de año que envió al pueblo salvadoreño la comandancia general del Frente Farabundo Martí para la Liberación Nacional en los albores de 1982 año de las grandes victorias.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-espanol/31-de-diciembre-1981-pm/canvas-1/canvas",
