  "items": [
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "31-de-diciembre-1981-am-canvas-1-entrevista-annotation-0.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "En estos momentos la unidad móvil de Radio Venceremos se encuentra con una señora víctima de la represión en El Mozote, jurisdicción de Meanguera, en el departamento de Morazán. Señora que logró sobrevivir a la masacre perpetrada por los cuerpos represivos en ese lugar y en otros caseríos del departamento.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Rufina_Amaya",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-espanol/31-de-diciembre-1981-am/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
          "t": "1449.0,1477.0"
        }
      }
    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "31-de-diciembre-1981-am-canvas-1-entrevista-annotation-1.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Compañera, ¿podría decirnos su nombre?",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Rufina_Amaya",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
