      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Rufina Amaya",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Rufina_Amaya",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
